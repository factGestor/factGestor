<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Es_upm_dit_isst_factGest</servlet-name>
		<servlet-class>es.upm.dit.isst.factGest.Es_upm_dit_isst_factGestServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Es_upm_dit_isst_factGest</servlet-name>
		<url-pattern>/es_upm_dit_isst_factgest</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Registro</servlet-name>
		<servlet-class>es.upm.dit.isst.factGest.RegistroServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registro</servlet-name>
		<url-pattern>/registrar</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Logueado</servlet-name>
		<servlet-class>es.upm.dit.isst.factGest.LogueadoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Logueado</servlet-name>
		<url-pattern>/logueado</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Confirmacion</servlet-name>
		<servlet-class>es.upm.dit.isst.factGest.ConfirmacionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Confirmacion</servlet-name>
		<url-pattern>/confirmacion</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
